    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^16.9.1",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",

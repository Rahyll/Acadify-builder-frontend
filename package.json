{
  "name": "oriental-college-admission",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/oriental-college-admission/server/main.js",
    "build:ssr": "ng build && ng run oriental-college-admission:server",
